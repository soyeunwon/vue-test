{"ast":null,"code":"import data from \"./assets/movies\";\nimport Navbar from \"./components/Navbar.vue\";\nimport Modal from \"./components/Modal.vue\";\nimport Event from \"./components/Event.vue\";\nimport Movies from \"./components/Movies.vue\";\nimport SearchBar from \"./components/SearchBar.vue\";\nexport default {\n  name: \"App\",\n  data() {\n    return {\n      isModal: false,\n      data,\n      data_temp: [...data],\n      selectedMovie: 0,\n      text: [\"1영화를 소개합니다.1\", \"2영화를 소개합니다.2\", \"3영화를 소개합니다.3\"],\n      eventTextNum: 0\n    };\n  },\n  methods: {\n    increaseLike(id) {\n      this.data.find(movie => {\n        if (movie.id === id) {\n          movie.like += 1;\n        }\n      });\n    },\n    searchMovie(title) {\n      this.data_temp = this.data.filter(movie => {\n        return movie.title.includes(title);\n      });\n    },\n    showAllMovie() {\n      this.data_temp = [...this.data];\n    }\n  },\n  components: {\n    Navbar: Navbar,\n    Modal: Modal,\n    Event: Event,\n    Movies: Movies,\n    SearchBar: SearchBar\n  },\n  mounted() {\n    console.log(\"mounted\");\n    setTimeout(() => {\n      this.eventTextNum += 1;\n    }, 3000);\n  }\n};","map":{"version":3,"names":["data","Navbar","Modal","Event","Movies","SearchBar","name","isModal","data_temp","selectedMovie","text","eventTextNum","methods","increaseLike","id","find","movie","like","searchMovie","title","filter","includes","showAllMovie","components","mounted","console","log","setTimeout"],"sources":["/Users/wonsoyeon/Desktop/vue/movie-info/src/App.vue"],"sourcesContent":["<template>\n  <Navbar />\n  <Event :text=\"text[eventTextNum]\" />\n  <SearchBar :data=\"data_temp\" @searchMovie=\"searchMovie($event)\" />\n  <p>\n    <button @click=\"showAllMovie\">전체보기</button>\n  </p>\n  <Movies\n    :data=\"data_temp\"\n    @openModal=\"\n      isModal = true;\n      selectedMovie = $event;\n    \"\n    @increaseLike=\"increaseLike($event)\"\n  />\n  <Modal\n    :data=\"data\"\n    :isModal=\"isModal\"\n    :selectedMovie=\"selectedMovie\"\n    @closeModal=\"isModal = false\"\n  />\n\n  <!-- <p v-for=\"(item, i) in foods\" :key=\"i\">{{ item }}</p> -->\n</template>\n\n<script>\nimport data from \"./assets/movies\";\nimport Navbar from \"./components/Navbar.vue\";\nimport Modal from \"./components/Modal.vue\";\nimport Event from \"./components/Event.vue\";\nimport Movies from \"./components/Movies.vue\";\nimport SearchBar from \"./components/SearchBar.vue\";\n\nexport default {\n  name: \"App\",\n  data() {\n    return {\n      isModal: false,\n      data,\n      data_temp: [...data],\n      selectedMovie: 0,\n      text: [\n        \"1영화를 소개합니다.1\",\n        \"2영화를 소개합니다.2\",\n        \"3영화를 소개합니다.3\",\n      ],\n      eventTextNum: 0,\n    };\n  },\n  methods: {\n    increaseLike(id) {\n      this.data.find((movie) => {\n        if (movie.id === id) {\n          movie.like += 1;\n        }\n      });\n    },\n    searchMovie(title) {\n      this.data_temp = this.data.filter((movie) => {\n        return movie.title.includes(title);\n      });\n    },\n    showAllMovie() {\n      this.data_temp = [...this.data];\n    },\n  },\n  components: {\n    Navbar: Navbar,\n    Modal: Modal,\n    Event: Event,\n    Movies: Movies,\n    SearchBar: SearchBar,\n  },\n  mounted() {\n    console.log(\"mounted\");\n    setTimeout(() => {\n      this.eventTextNum += 1;\n    }, 3000);\n  },\n};\n</script>\n\n<style>\n* {\n  box-sizing: border-box;\n  margin: 0;\n}\n\nbody {\n  max-width: 768px;\n  margin: 0 auto;\n  padding: 20px;\n}\n\nh1,\nh2,\nh3 {\n  margin-bottom: 1rem;\n}\n\np {\n  margin-bottom: 0.5rem;\n}\n\nbutton {\n  margin-right: 10px;\n  margin-top: 1rem;\n}\n\n.item {\n  width: 100%;\n  border: 1px solid #ccc;\n  display: flex;\n  margin-bottom: 20px;\n  padding: 1rem;\n}\n\n.item figure {\n  width: 30%;\n  margin-right: 1rem;\n}\n\n.item img {\n  width: 100%;\n}\n\n.item .info {\n  width: 100%;\n}\n\n.modal {\n  background: rgba(0, 0, 0, 0.7);\n  position: fixed;\n  left: 0;\n  top: 0;\n  width: 100%;\n  height: 100vh;\n  display: flex;\n  justify-content: center;\n  align-items: center;\n}\n\n.modal .inner {\n  background: #fff;\n  width: 80%;\n  padding: 20px;\n  border-radius: 10px;\n}\n</style>\n"],"mappings":"AA0BA,OAAOA,IAAG,MAAO,iBAAiB;AAClC,OAAOC,MAAK,MAAO,yBAAyB;AAC5C,OAAOC,KAAI,MAAO,wBAAwB;AAC1C,OAAOC,KAAI,MAAO,wBAAwB;AAC1C,OAAOC,MAAK,MAAO,yBAAyB;AAC5C,OAAOC,SAAQ,MAAO,4BAA4B;AAElD,eAAe;EACbC,IAAI,EAAE,KAAK;EACXN,IAAIA,CAAA,EAAG;IACL,OAAO;MACLO,OAAO,EAAE,KAAK;MACdP,IAAI;MACJQ,SAAS,EAAE,CAAC,GAAGR,IAAI,CAAC;MACpBS,aAAa,EAAE,CAAC;MAChBC,IAAI,EAAE,CACJ,cAAc,EACd,cAAc,EACd,cAAc,CACf;MACDC,YAAY,EAAE;IAChB,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,YAAYA,CAACC,EAAE,EAAE;MACf,IAAI,CAACd,IAAI,CAACe,IAAI,CAAEC,KAAK,IAAK;QACxB,IAAIA,KAAK,CAACF,EAAC,KAAMA,EAAE,EAAE;UACnBE,KAAK,CAACC,IAAG,IAAK,CAAC;QACjB;MACF,CAAC,CAAC;IACJ,CAAC;IACDC,WAAWA,CAACC,KAAK,EAAE;MACjB,IAAI,CAACX,SAAQ,GAAI,IAAI,CAACR,IAAI,CAACoB,MAAM,CAAEJ,KAAK,IAAK;QAC3C,OAAOA,KAAK,CAACG,KAAK,CAACE,QAAQ,CAACF,KAAK,CAAC;MACpC,CAAC,CAAC;IACJ,CAAC;IACDG,YAAYA,CAAA,EAAG;MACb,IAAI,CAACd,SAAQ,GAAI,CAAC,GAAG,IAAI,CAACR,IAAI,CAAC;IACjC;EACF,CAAC;EACDuB,UAAU,EAAE;IACVtB,MAAM,EAAEA,MAAM;IACdC,KAAK,EAAEA,KAAK;IACZC,KAAK,EAAEA,KAAK;IACZC,MAAM,EAAEA,MAAM;IACdC,SAAS,EAAEA;EACb,CAAC;EACDmB,OAAOA,CAAA,EAAG;IACRC,OAAO,CAACC,GAAG,CAAC,SAAS,CAAC;IACtBC,UAAU,CAAC,MAAM;MACf,IAAI,CAAChB,YAAW,IAAK,CAAC;IACxB,CAAC,EAAE,IAAI,CAAC;EACV;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}